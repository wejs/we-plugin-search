{
  "name": "we-plugin-search",
  "description": "We.js plugin for automaticaly parse query params to database search",
  "version": "2.0.0",
  "homepage": "https://wejs.org",
  "main": "plugin.js",
  "scripts": {
    "test": "NODE_ENV=test LOG_LV=info ./node_modules/.bin/mocha test/bootstrap.js test/**/*.test.js",
    "coverage": "NODE_ENV=test LOG_LV=info node_modules/.bin/istanbul cover -x src node_modules/.bin/_mocha -- -u exports -R spec test/bootstrap.js test/**/*.test.js",
    "postversion": "git push"
  },
  "devDependencies": {
    "express-mysql-session": "^1.2.3",
    "fs-extra": "^5.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.0.1",
    "mysql2": "^1.5.2",
    "ncp": "^2.0.0",
    "rimraf": "^2.6.2",
    "supertest": "^3.0.0",
    "we-core": "^2",
    "we-test-tools": "wejs/we-test-tools"
  },
  "repository": "wejs/we-plugin-search",
  "author": "Alberto Souza <contato@albertosouza.net>",
  "license": "MIT",
  "keywords": [
    "wejs-plugin",
    "search"
  ],
  "engines": {
    "node": ">=6.0.0"
  },
  "wejs": {
    "plugins": {
      "we-plugin-post": true
    }
  }
}
